{
  "name": "sidebarjs",
  "library": "SidebarJS",
  "version": "6.0.0",
  "description": "Create mobile sidebar/sidenav experience in pure javascript",
  "main": "./lib/umd/sidebarjs.js",
  "module": "./lib/esm/sidebarjs.js",
  "typings": "./lib/src/index.d.ts",
  "scripts": {
    "test": "jest",
    "build": "node --experimental-modules .build/index.mjs",
    "build:watch": "npm run build -- --watch",
    "server": "live-server --open=demo",
    "demo": "npm run build && npm run server",
    "start": "npm run build:watch | npm run server",
    "format": "prettier --config .prettierrc --write src/*.ts && tslint -p tsconfig.json",
    "changelog": "conventional-changelog -i CHANGELOG.md -s -r 0",
    "commit:version": "git add . && git commit -am 'chore(version): $npm_package_version'",
    "prerelease": "npm run build && npm run changelog && npm run commit:version",
    "release": "git tag $npm_package_version && git push --tags && npm publish"
  },
  "engines": {
    "npm": ">=6.10.0",
    "node": ">=12.7.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SidebarJS/sidebarjs.git"
  },
  "author": "Lorenzo D'Ianni <lorenzodianni@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/SidebarJS/sidebarjs/issues"
  },
  "homepage": "https://github.com/SidebarJS/sidebarjs#readme",
  "keywords": [
    "sidebar",
    "sidenav",
    "javascript",
    "sidebarjs"
  ],
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@types/jest": "^24.0.16",
    "autoprefixer": "^9.6.1",
    "conventional-changelog-cli": "^2.0.23",
    "cssnano": "^4.1.10",
    "fs-extra": "^8.1.0",
    "gesture-events": "^1.0.0",
    "jest": "^24.8.0",
    "live-server": "^1.2.1",
    "node-sass": "^4.12.0",
    "node-sass-watcher": "^0.5.1",
    "postcss": "^7.0.17",
    "prettier": "^1.18.2",
    "rollup": "^1.18.0",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-commonjs": "^10.0.2",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-terser": "^5.1.1",
    "signale": "^1.4.0",
    "sinon": "^7.3.2",
    "ts-jest": "^24.0.2",
    "tslint": "^5.18.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.5.3",
    "yargs": "^13.3.0"
  },
  "browserslist": [
    "iOS 6",
    "Android 4",
    "last 2 versions"
  ],
  "jest": {
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ]
  }
}
