{
  "name": "sidebarjs",
  "library": "SidebarJS",
  "version": "5.4.0",
  "description": "Create mobile sidebar/sidenav experience in pure javascript",
  "main": "./lib/umd/sidebarjs.js",
  "typings": "./lib/src/index.d.ts",
  "scripts": {
    "test": "jest",
    "build": "node --experimental-modules .build",
    "build:watch": "npm run build -- --watch",
    "server": "live-server --open=demo",
    "demo": "npm run build && npm run server",
    "start": "npm run build:watch | npm run server",
    "format": "prettier --config .prettierrc --write src/*.ts && tslint -p tsconfig.json",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SidebarJS/sidebarjs.git"
  },
  "author": "Lorenzo D'Ianni <lorenzodianni@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/SidebarJS/sidebarjs/issues"
  },
  "homepage": "https://github.com/SidebarJS/sidebarjs#readme",
  "keywords": [
    "sidebar",
    "sidenav",
    "javascript",
    "sidebarjs"
  ],
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.2.3",
    "@types/jest": "^23.3.12",
    "autoprefixer": "^9.4.4",
    "conventional-changelog-cli": "^2.0.11",
    "cssnano": "^4.1.8",
    "fs-extra": "^7.0.1",
    "gesture-events": "^1.0.0",
    "jest": "^23.6.0",
    "live-server": "^1.2.1",
    "node-sass": "^4.11.0",
    "node-sass-watcher": "^0.5.1",
    "postcss": "^7.0.10",
    "prettier": "^1.15.3",
    "rollup": "^1.1.0",
    "rollup-plugin-babel": "^4.3.0",
    "rollup-plugin-commonjs": "^9.2.0",
    "rollup-plugin-node-resolve": "^4.0.0",
    "rollup-plugin-terser": "^4.0.2",
    "signale": "^1.3.0",
    "sinon": "^7.2.2",
    "ts-jest": "^23.10.5",
    "tslint": "^5.12.1",
    "tslint-config-prettier": "^1.17.0",
    "typescript": "^3.2.2",
    "yargs": "^12.0.5"
  },
  "browserslist": [
    "iOS 6",
    "Android 4",
    "last 2 versions"
  ],
  "jest": {
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ]
  }
}
